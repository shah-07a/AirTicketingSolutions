    string filtered_search_response = string.Empty;
    string stopfilters = string.Empty;
    string preferredAirlines = string.Empty;
    string distinctAirlines = string.Empty;
    if (ViewBag.ResponseResult != null)
    {
        filtered_search_response = (string)ViewBag.ResponseResult;
    }
    if (ViewBag.StopFilters != null)
    {
        stopfilters = (string)ViewBag.StopFilters;
    }
    if (ViewBag.PreferredAirlines != null)
